const usersData = require('../seed-data/products');
const postsData = require('../seed-data/reviews');

exports.seed = function (knex) {
  return knex('review')
    .del()
    .then(function () {
      return knex('product').del();
    })
    .then(function () {
      return knex('product').insert(usersData);
    })
    .then(() => {
      return knex('review').insert(postsData);
    });
};